{
  "description": "Schema for engagement description output.",
  "type": "object",
  "properties": {
    "RationaleOriginalVersion": {
      "type": "string",
      "description": "Plain-text rationale for the OriginalVersion."
    },
    "OriginalVersion": {
      "type": "string",
      "description": "Original engagement description in HTML."
    },
    "RationaleCleanVersions": {
      "type": "string",
      "description": "Brief rationale shared across clean versions (Bullet, Mixed, Paragraph)."
    },
    "BulletVersion": {
      "type": "string",
      "description": "Bullet engagement description in HTML."
    },
    "MixedVersion": {
      "type": "string",
      "description": "Mixed engagement description in HTML."
    },
    "ParagraphVersion": {
      "type": "string",
      "description": "Paragraph engagement description in HTML."
    }
  },
  "required": [
    "RationaleOriginalVersion",
    "OriginalVersion",
    "RationaleCleanVersions",
    "BulletVersion",
    "MixedVersion",
    "ParagraphVersion"
  ]
}
